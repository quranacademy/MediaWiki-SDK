{
    "name": "quranacademy/mediawiki-sdk",
    "description": "SDK for MediaWiki API",
    "type": "library",
    "keywords": ["mediawiki"],
    "license": "MIT",
    "homepage": "https://github.com/quranacademy/MediaWiki-SDK",
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "symfony/console": "~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.2",
        "mikey179/vfsstream": "^1.6",
        "friendsofphp/php-cs-fixer": "^2.15",
        "illuminate/contracts": "^6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "authors": [
        {
            "name": "Imangazaliev Muhammad",
            "email": "imangazalievm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MediaWiki\\": "src/MediaWiki/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MediaWiki\\Tests\\": "tests/"
        }
    }
}
